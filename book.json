{
  "title": "LeetCode",
  "description": "记录 LeetCode刷题过程",
  "author": "gaohueric",
  "output.name": "site",
  "language": "zh-hans",
  "root": ".",
  "links": {
    "sidebar": {
      "技术博客": "https://gaohueric.github.io/"
    }
  },
  "plugins": [
    "chapter-fold",
    "-lunr",
    "-search",
    "search-plus",
    "splitter",
    "anchor-navigation-ex",
    "tbfed-pagefooter",
    "disqus",
    "edit-link",
    "puml",
    "donate",
    "code",
    "pageview-count",
    "hide-element",
    "popup",
    "terminal",
    "alerts",
    "katex"
  ],
  "pluginsConfig": {
    "anchor-navigation-ex": {
      "isShowTocTitleIcon": true,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
    },
    "tbfed-pagefooter": {
      "copyright": "&copy gaohueric",
      "modify_label": "文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "disqus": {
      "shortName": "gaohuericgitbook"
    },
    "edit-link": {
      "base": "https://github.com/gaohueric/gitbook/blob/main",
      "label": "编辑本页"
    },
    "donate": {
      "wechat": "https://raw.githubusercontent.com/gaohueric/blogpicture/master/wechat.png",
      "alipay": "https://raw.githubusercontent.com/gaohueric/blogpicture/master/alibabapay.png",
      "title": "",
      "button": "赏",
      "alipayText": "支付宝打赏",
      "wechatText": "微信打赏"
    },
    "hide-element": {
      "elements": [
        ".gitbook-link"
      ]
    },
    "terminal": {
      "copyButtons": true,
      "fade": false,
      "style": "flat"
    }
  }
}
